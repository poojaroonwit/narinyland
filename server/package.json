{
    "name": "narinyland-server",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:seed": "tsx prisma/seed.ts",
        "db:studio": "prisma studio",
        "db:rls": "tsx prisma/enable-rls.ts",
        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.700.0",
        "@aws-sdk/s3-request-presigner": "^3.700.0",
        "@prisma/client": "^6.19.2",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "multer": "^1.4.5-lts.2",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.14.0",
        "@types/uuid": "^10.0.0",
        "prisma": "^6.4.1",
        "tsx": "^4.19.2",
        "typescript": "~5.8.2"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    }
}